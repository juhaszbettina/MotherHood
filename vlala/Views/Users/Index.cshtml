@model IEnumerable<MotherHood.Models.ApplicationUser>

@{
    ViewData["Title"] = "Felhasználók";
}

<h1>Felhasználók</h1>

<p>
    
</p>
<div class="card-deck">
    @foreach (var user in Model)
    {
    <a asp-action="Details" asp-route-id="@user.Id" class="card">
        <div class="card-body row">
            <div class="col-12">
                @user.Email
            </div>
            <div class="col-12">
                @user.lastName @user.firstName
            </div>          
            <div class="col-12">
                @user.Megye.Nev
            </div>
        </div>
    </a>
        
    }
</div>
